datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  user_id Int @id @default(autoincrement())
  email String @unique
  password String
  phone_number String
  role String
  status String? //(Active, Inactive)
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
